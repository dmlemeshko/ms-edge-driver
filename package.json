{
  "name": "ms-chromium-edge-driver",
  "version": "0.4.1",
  "keywords": [
    "edge",
    "edgedriver",
    "msedgedriver",
    "webdriver",
    "selenium"
  ],
  "description": "NPM wrapper for Webdriver Chromium-based Edge driver",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": "git@github.com:dmlemeshko/ms-edge-driver.git",
  "author": "<dima.inspired@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "node lib/install.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rm -rf lib bin msedgedriver.zip paths.json && tsc -p .",
    "buildWin": "del /s/q lib bin msedgedriver.zip paths.json & tsc -p .",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "dependencies": {
    "extract-zip": "^2.0.1",
    "got": "^11.8.2",
    "lodash": "^4.17.21",
    "regedit": "^3.0.3"
  },
  "devDependencies": {
    "@types/extract-zip": "^1.6.2",
    "@types/got": "^9.6.11",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node":"^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-mock-process": "^1.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
