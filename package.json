{
  "name": "ms-chromium-edge-driver",
  "version": "0.2.5",
  "keywords": [
    "edge",
    "edgedriver",
    "msedgedriver",
    "webdriver",
    "selenium"
  ],
  "description": "NPM wrapper for Webdriver Chromium-based Edge driver",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": "git@github.com:dmlemeshko/ms-edge-driver.git",
  "author": "<dima.inspired@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "node lib/install.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "rm -rf lib bin msedgedriver.zip paths.json && tsc -p .",
    "buildWin": "del /s/q lib bin msedgedriver.zip paths.json & tsc -p .",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": ["precommit-msg", "lint"],
  "dependencies": {
    "extract-zip": "^2.0.0",
    "got": "^11.3.0",
    "lodash": "^4.17.15",
    "regedit": "^3.0.3",
    "util": "^0.12.2"
  },
  "devDependencies": {
    "@types/extract-zip": "^1.6.2",
    "@types/got": "^9.6.9",
    "@types/jest": "^26.0.10",
    "@types/lodash":"^4.14.149",
    "@types/node": "^14.0.11",
    "jest": "^26.4.2",
    "jest-mock-process": "^1.4.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  }
}
